{
  "expo": {
    "name": "Brainiac",
    "slug": "brainiac",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/images/brainiac.png",
    "scheme": "brainiac",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "assets/images/brainiac.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lewibelayneh.brainiac",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/brainiac.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lewibelayneh.brainiac",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "assets/images/brainiac.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "af1f4808-433d-4762-94fe-7ec9d3d3d5be"
      }
    }
  }
}
